ALTER SESSION SET NLS_DATE_FORMAT='DD/MM/YYYY HH24:MI:SS'; -- 26/01/2022 10:44:53
ALTER SESSION SET NLS_DATE_FORMAT='DD/MM/YYYY'; -- 26/01/2022
ALTER SESSION SET NLS_DATE_FORMAT='DD/MM/YY'; -- 26/01/22

ALTER SESSION SET NLS_TIMESTAMP_FORMAT = 'DD/MM/RR HH24:MI:SS.FF'; -- 26/01/22 10:45:24.000000000
ALTER SESSION SET NLS_TIMESTAMP_FORMAT = 'DD/MM/RR HH24:MI:SS'; -- 26/01/22 10:48:35
